{
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "baseUrl": "./",
    "paths": {}
  },
  "include": [
    "frontend/src/**/*"
  ]
}